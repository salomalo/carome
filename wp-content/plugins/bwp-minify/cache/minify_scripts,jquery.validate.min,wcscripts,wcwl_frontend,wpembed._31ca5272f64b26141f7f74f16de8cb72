function replaceUrlParam(url,paramName,paramValue){if(paramValue==null)paramValue='';var pattern=new RegExp('\\b('+paramName+'=).*?(&|$)')
if(url.search(pattern)>=0){return url.replace(pattern,'$1'+paramValue+'$2');}
